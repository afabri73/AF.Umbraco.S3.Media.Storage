{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "AWS": {
    "Profile": "YOUR_AWS_PROFILE",
    "Region": "YOUR_AWS_REGION",
    "ServiceURL": "https://s3.YOUR_AWS_REGION.amazonaws.com",
    "ForcePathStyle": false
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "MacroErrors": "Throw"
      },
      "Hosting": {
        "Debug": true
      }
    },
    "Storage": {
      "AWSS3": {
        "Media": {
          "BucketName": "YOUR_BUCKET_NAME",
          "Region": "YOUR_AWS_REGION",
          "CacheRetention": {
            "Enabled": false,
            "NumberOfDays": 90,
            "TestModeEnable": true,
            "TestModeSweepSeconds": 30,
            "TestModeMaxAgeMinutes": 10
          }
        }
      }
    }
  }
}
